<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Renta_Carros"
            xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
            android:TabbedPage.ToolbarPlacement="Bottom"
            BarBackgroundColor="#5f5f5f"
            BarTextColor="White"
            x:Class="Renta_Carros.Menu"
            Title="Menu"
            x:Name="MenuTabbedPage">

    <local:Inicio Title="Inicio" IconImageSource="{FontImage FontFamily='fas', Glyph='&#xf015;', Size='30'}"></local:Inicio>
    <local:Prueba Title="Lista"  IconImageSource="{FontImage FontFamily='fas', Glyph='&#xf03a;', Size='30'}"></local:Prueba>
    
    <local:RentarCarro Title="Rentar" IconImageSource="{FontImage FontFamily='fas', Glyph='&#xf07a;', Size='30'}"></local:RentarCarro>
    <local:AgregarCarro Title="Agregar" IconImageSource="{FontImage FontFamily='fas', Glyph='&#xf1b9;', Size='30'}"></local:AgregarCarro>
    
    

</TabbedPage>